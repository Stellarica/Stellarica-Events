{
  "schema_version": 1,
  "quilt_loader": {
    "group": "net.stellarica.oxidiser",
    "id": "oxidiser",
    "version": "${version}",
    "metadata": {
      "name": "Oxidiser",
      "description": "Server-side event library for Quilt. Fork of NucleoidMC's Stimuli",
      "contributors": {
        "trainb0y": "Maintainer",
        "Nucleoid Contributors": "Creation of Stimuli"
      },
      "contact": {
        "homepage": "https://stellarica.net/",
        "issues": "https://github.com/Stellarica/Oxidiser/issues",
        "sources": "https://github.com/Stellarica/Oxidiser"
      },
      "icon": "assets/oxidiser/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "server_init": {
        "adapter": "kotlin",
        "value": "net.stellarica.oxidiser.OxidiserInitializer"
      },
      "pre_launch": {
        "adapter": "kotlin",
        "value": "net.stellarica.oxidiser.OxidiserPreLaunchInitializer"
      }
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.17.6-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=5.0.0-"
      },
      {
        "id": "qkl",
        "versions": ">=0.1.4-"
      },
      {
        "id": "minecraft",
        "versions": ">=1.19.3"
      }
    ]
  },
  "mixin": "oxidiser.mixins.json"
}